{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jquery": true
    },
    "globals": {
      "moment": "false",
      "SocialButtonBar": "false",
      "Tooltip": "false",
      "Table": "false",
      "TextSearch": "false",
      "noUiSlider": "false",
      "Papa": "false",
      "Record": "false"
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "semi": 2,
      /* for interim changed args to none to quiet checker during switch over 
      to eslint from jshint. deal with this later */
      "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
      /* ditto for no-mixed-spaces-and-tabs */
      "no-mixed-spaces-and-tabs": 0
    }
}
